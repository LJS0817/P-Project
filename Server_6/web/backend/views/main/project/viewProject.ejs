<link rel="stylesheet" href="/css/main/listview.css"/>
<link rel="stylesheet" href="/css/main/project/viewProject.css"/>
<script>
    <% let text = [
        'historyFilterPage',
        'listPageOptionReset',
        'listPageOptionDownload',
        'delete',
        'viewProjectPageTitle',
        'name',
        'listPageResetPopupMsg',
        'listPageResetPopupDetail',
        'listPageDeletePopupMsg',
        'listPageDeletePopupDetail',
        'listPageNeedSelectMsg',
        'viewProjectPageFailedDownload'
    ] %>
    <% text.forEach(str => { %>
        document.textData['<%= str %>'] = '<%= __(str) %>';
    <% }); %>
    document.textData['viewProjectAlreadyAnaylzedMsg'] = '<%- __("viewProjectAlreadyAnaylzedMsg") %>';
    document.textData['viewProjectPageNoImageMsg'] = '<%- __("viewProjectPageNoImageMsg") %>';
</script>
<script src="/js/main/project/analysisProject.js" defer></script>
<script src="/js/main/scroll.js" defer></script>
<script src="/js/main/sort.js" defer></script>
<div class="viewProject">
    <div class="page">
        <div class="pageTitle">
            <h2></h2>
            <div class="pageScores">
                <p><%= __('viewProjectScoreTitle') %></p>
                <div class="verticalLine"></div>
                <div class="score balance">
                    <p><%= __('viewProjectBalanceTitle') %></p>
                    <p></p>
                    <i class="fi fi-rr-interrogation help">
                        <div class="helpContent"><%- __('viewProjectBalanceDesc') %></div>
                    </i>
                </div>
                <div class="verticalLine"></div>
                <div class="score focus">
                    <p><%= __('viewProjectFocusTitle') %></p>
                    <p></p>
                    <i class="fi fi-rr-interrogation help">
                        <div class="helpContent"><%- __('viewProjectFocusDesc') %></div>
                    </i>
                </div>
            </div>
        </div>
        <div class="result">
            <div class="dropHere">
                <svg xmlns="http://www.w3.org/2000/svg" id="Layer_1" data-name="Layer 1" viewBox="0 0 24 24"><path d="M17.974,7.146c-.331-.066-.602-.273-.742-.569-1.55-3.271-5.143-5.1-8.734-4.438-3.272,.6-5.837,3.212-6.384,6.501-.162,.971-.15,1.943,.033,2.89,.06,.309-.073,.653-.346,.901-1.145,1.041-1.801,2.524-1.801,4.07,0,3.032,2.467,5.5,5.5,5.5h11c4.136,0,7.5-3.364,7.5-7.5,0-3.565-2.534-6.658-6.026-7.354Zm-1.474,12.854H5.5c-1.93,0-3.5-1.57-3.5-3.5,0-.983,.418-1.928,1.146-2.59,.786-.715,1.155-1.773,.963-2.763-.138-.712-.146-1.445-.024-2.181,.403-2.422,2.365-4.421,4.771-4.862,.385-.07,.768-.104,1.146-.104,2.312,0,4.405,1.289,5.422,3.434,.413,.872,1.2,1.482,2.158,1.673,2.56,.511,4.417,2.779,4.417,5.394,0,3.032-2.468,5.5-5.5,5.5Zm-1.379-7.707c.391,.391,.391,1.023,0,1.414-.195,.195-.451,.293-.707,.293s-.512-.098-.707-.293l-1.707-1.707v5c0,.553-.448,1-1,1s-1-.447-1-1v-5l-1.707,1.707c-.391,.391-1.023,.391-1.414,0s-.391-1.023,0-1.414l2.707-2.707c.386-.386,.893-.58,1.4-.583l.014-.003,.014,.003c.508,.003,1.014,.197,1.4,.583l2.707,2.707Z"/></svg>
                <h3><%= __('viewProjectDropHereTitle') %></h3>
                <!-- <h4>to upload to your deisgn<br>( .png, .jpg, .jpeg, HTML/CSS Folder )</h4> -->
                <h4><%= __('viewProjectDropHereSubtitle') %><br>( .png, .jpg, .jpeg )</h4>
            </div>
            <input type="hidden" id="currentProjectId" value="<%= projectId %>"> <%# 프로젝트 ID도 같이 전송되도록 추가%>
            <div class="uiGroup">
                <div class="btnContainer">
                    <div class="ui visibleBtn target-path active">
                        <i class="fi fi-rr-code-branch"></i>
                        <i class="fi fi-rr-slash"></i>
                    </div>
                    <div class="ui visibleBtn target-heatmap active">
                        <i class="fi fi-rr-eye"></i>
                        <i class="fi fi-rr-slash"></i>
                    </div>
                    <div class="ui visibleBtn target-heatmap half">
                        <i class="fi fi-rr-low-vision"></i>
                    </div>
                </div>
                <div class="btnContainer">
                    <div class="ui addFile">
                        <input type="file" name="fileData" id="fileData" multiple>
                        <div class="fileInput"><i class="fi fi-rr-replace"></i></div>
                    </div>
                    <div class="ui sendDesign">
                        <i class="fi fi-rr-analyse"></i>
                        <p><%= __('viewProjectAnalysisButtonMsg') %></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="pageList">
        <h2><%= __('viewProjectPageTitle') %> <p id="addPageBtn"><i class="fi fi-rr-plus"></i>&emsp;<%= __('viewProjectAddPageButtonMsg') %></p></h2>
        <div class="verticalScrollableList">
            <div class="tableContainer listParent">
                <div class="categories linked">
                    <p class="area"></p>
                    <p class="area sortable"><%= __('listMsg_name') %><span class="arrow">▼</span></p>
                    <p class="area"><%= __('listMsg_analysis') %></p>
                    <p class="area sortable"><%= __('listMsg_updated') %><span class="arrow">▼</span></p>
                    <p class="area"><%= __('listMsg_option') %></p>
                </div>
                <div class="listContainer">
                    <div class="list">
                        <% pages.forEach(page => { %>
                            <%- include('pageElement', {
                                id: page.id,
                                name: page.name,
                                analyzed: page.analyzed,
                                datetime: page.datetime,
                            }) %>
                        <% }); %>
                    </div>
                </div>
            </div>
            <div class="scrollTrack">
                <div class="scrollBar" id="customScroll"></div>
            </div>
        </div>
    </div>
</div>